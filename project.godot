; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_class_icons={

}
_global_script_classes=[  ]

[application]

config/name="DS"
run/main_scene="res://scenes/root.tscn"
boot_splash/image="res://textures_manual/logo/black_pixel.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://textures_free/icons/icon_128.png"

[autoload]

Logger="*res://scripts/godot-logger/logger.gd"
Settings="*res://scripts/singletons/settings.gd"
Debug="*res://scripts/singletons/debug.gd"
Geometry="*res://scripts/singletons/geometry.gd"
Util="*res://scripts/singletons/util.gd"
ImportantRandomizer="*res://scripts/randomizer.gd"
AudioPlayer="*res://scenes/singletons/audio_player.tscn"
NonImportantRandomizer="*res://scripts/randomizer.gd"
TransitionScene="*res://scenes/singletons/transition_scene.scn"
Layers="*res://scripts/singletons/layers.gd"
CardDesc="*res://scripts/singletons/card_desc.gd"
FontManager="*res://scripts/singletons/font_manager.gd"

[debug]

gdscript/warnings/unused_variable=false
gdscript/warnings/unused_class_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/return_value_discarded=false

[display]

window/vsync/use_vsync=false

[editor_plugins]

enabled=PoolStringArray( "codeandweb.texturepacker" )

[global]

_global_script_classes=[  ]

